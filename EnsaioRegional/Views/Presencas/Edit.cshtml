@model EnsaioRegional.Models.Presenca

@{
    ViewData["Title"] = "Edit - Presença";
}

<h1>Edit - Presença</h1>

<h4>Presenca</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
