            <input type="hidden" asp-for="IdPresenca" />
            <div class="form-group">
                <label asp-for="IdData" class="control-label"></label>
                <select asp-for="IdData" class="form-control" asp-items="ViewBag.IdData"></select>
                <span asp-validation-for="IdData" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdMusico" class="control-label"></label>
                <select asp-for="IdMusico" class="form-control" asp-items="ViewBag.IdMusico"></select>
                <span asp-validation-for="IdMusico" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
